<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.helloworld" version="1.0.0">
  <name>NBM-STAR</name>
  <description>Первые в космосе</description>
  <author href="http://phonegap.com" email="support@phonegap.com">PhoneGap Team</author>
  <content src="index.html"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="14"/>
  <preference name="phonegap-version" value="cli-6.4.0" />
  <preference name="windows-arch" value="x86" />
  <preference name="windows-identity-name" value="PhonegapBuild.PhoneGapBuild" />
  <preference name="Fullscreen" value="true" />
  <preference name="Orientation" value="portrait" />
  <icon src="icon2.png"/>
  <platform name="android">

  </platform>
  <platform name="wp8">

  </platform>
  <platform name="windows">

  </platform>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
